  "dependencies": {
    "@astrojs/netlify": "^3.0.0",
    "@astrojs/starlight": "^0.9.0",
    "astro": "^3.0.7",
    "prettier": "^3.0.3",
    "sharp": "^0.32.3"
  },
  "devDependencies": {
    "markdownlint": "^0.31.0"
  }
}
